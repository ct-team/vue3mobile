<!DOCTYPE html>
<html lang="">
  <head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <title>demo</title>
    <meta http-equiv="expires" content="0" />
    <meta http-equiv="Pragma" content="no-cache" />
    <meta http-equiv="Cache-control" content="no-cache" />
    <meta content="telephone=no,email=no" name="format-detection" />
    <meta name="apple-mobile-web-app-capable" content="yes" />
    <meta
      name="ctEnv"
      content="<%=process.env.VUE_APP_ENV || process.env.NODE_ENV%>"
    />
    <% if(process.env.NODE_ENV !== "development") { %>
    <meta name="AppPublic" content="<ctAppPublic>" />
    <% } %>
    <meta
      name="viewport"
      content="initial-scale=1,maximum-scale=1, minimum-scale=1"
    />
    <link
      rel="shortcut icon"
      href="//static.tcy365.com/uc/common/images/favicon.ico"
    />
    <script>
      !(function (e, t) {
        var n = t.documentElement,
          d = e.devicePixelRatio || 1;

        function i() {
          var w =
            n.clientWidth > n.clientHeight ? n.clientHeight : n.clientWidth;
          var CW = w < 320 ? 320 : w;
          var e = CW / 7.5;
          n.style.fontSize = e + 'px';
        }
        if (
          ((function e() {
            t.body
              ? (t.body.style.fontSize = '16px')
              : t.addEventListener('DOMContentLoaded', e);
          })(),
          i(),
          // e.addEventListener('resize', i),
          e.addEventListener('pageshow', function (e) {
            e.persisted && i();
          }),
          d >= 2)
        ) {
          var o = t.createElement('body'),
            a = t.createElement('div');
          (a.style.border = '.5px solid transparent'),
            o.appendChild(a),
            n.appendChild(o),
            1 === a.offsetHeight && n.classList.add('hairlines'),
            n.removeChild(o);
        }
      })(window, document);
    </script>
    <% for (var css in htmlWebpackPlugin.files.css) { %>
    <link href="<%=htmlWebpackPlugin.files.css[css] %>" rel="stylesheet" />
    <% } %>
  </head>
  <body>
    <div id="app"></div>

    <!-- start 不要修改 -->
    <!-- built files will be auto injected 不要修改 -->
    <% for (var chunk in htmlWebpackPlugin.files.chunks) { %>
    <script
      type="text/javascript"
      src="<%=htmlWebpackPlugin.files.chunks[chunk].entry %>"
    ></script>
    <% } %>
    <!-- end 不要修改 -->
  </body>
</html>
